                    li.nav-item
                      a(href='about-us.html') About Us
                li.nav-item
                  a.page-scroll(href=baseURL+"#team") Team
                //- li.nav-item
                  a.page-scroll(href=baseURL+"#contact") Contact
                li.nav-item()
                  a(href=baseURL+"/article" class={"active" : pageActive === 'article'}) Articles
